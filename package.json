    "electron-builder": "^20.28.4",
    "electron-reload": "^1.2.5",
    "less": "^3.8.1",
    "mocha": "^5.2.0",
    "parcel-bundler": "^1.10.3",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "convert-units": "^2.3.4",
    "electron": "^3.0.5",
